{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": 0,
      "no-use-before-define": 0,
      "semi": [2, "never"],
      "jsx-quotes": ["error", "prefer-single"],
      "arrow-parens": [2, "as-needed"],
      "react/prop-types": [0],
      "react/require-default-props": [0],
      "react/prefer-stateless-function": 0
    }
}
